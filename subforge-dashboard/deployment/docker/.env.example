# Production Environment Variables for SubForge Dashboard
# Copy this file to .env and update with your actual values

# Version
VERSION=v1.0.0

# Database Configuration
POSTGRES_DB=subforge
POSTGRES_USER=subforge
POSTGRES_PASSWORD=your_secure_postgres_password_here

# Redis Configuration
REDIS_PASSWORD=your_secure_redis_password_here

# Backend Configuration
SECRET_KEY=your_very_long_and_secure_secret_key_here_at_least_32_characters
CORS_ORIGINS=https://subforge.yourdomain.com,https://app.yourdomain.com
LOG_LEVEL=info
BACKEND_WORKERS=4

# Frontend Configuration
NEXT_PUBLIC_API_URL=https://api.subforge.yourdomain.com
NEXT_PUBLIC_WS_URL=wss://api.subforge.yourdomain.com

# Monitoring and Logging
SENTRY_DSN=your_sentry_dsn_here

# SSL Configuration
SSL_CERT_PATH=/etc/nginx/ssl/cert.pem
SSL_KEY_PATH=/etc/nginx/ssl/key.pem

# Backup Configuration
BACKUP_S3_BUCKET=subforge-backups
BACKUP_RETENTION_DAYS=30
AWS_ACCESS_KEY_ID=your_aws_key
AWS_SECRET_ACCESS_KEY=your_aws_secret

# Domain Configuration
DOMAIN=subforge.yourdomain.com
API_DOMAIN=api.subforge.yourdomain.com

# Resource Limits
POSTGRES_MAX_CONNECTIONS=200
REDIS_MAX_MEMORY=512mb
BACKEND_MAX_WORKERS=8
FRONTEND_REPLICAS=2